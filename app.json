{
  "expo": {
    "name": "V.I.P Scanner",
    "slug": "vip-scanner",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-adab.png",
    "scheme": "vipnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-adab.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.ipost.vipscanner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-adab.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "20d92038-662d-4c5e-afbe-0467f5264194"
      }
    }
  }
}
